  "description": "richtext-source-generator",
  "main": "index.html",
  "scripts": {
    "start": "npm run build",
    "dev": "NODE_ENV=development node -r @babel/register ./node_modules/.bin/webpack-dev-server --config ./webpack/dev.js --mode development  --open --watch --colors",
